import java.util.*;
class Main {
     static int array[]={21,54,23,78,98,148};
    public static void main(String[] args) {
      int maxNo= findLargestNumber();
      System.out.println("Largest Number in the array is " +maxNo);
    }

    private static int findLargestNumber()
    {
        int max = array[0];
        for (int i=0; i<array.length; i++)
        {
            if (array[i]>max)
                max = array[i];
        }
         return max;
    }
}

    
    
