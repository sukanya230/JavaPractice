import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Scanner sc1 = new Scanner(System.in);
        System.out.println("Enter the number :");
        int a=sc.nextInt();
        System.out.println("Given Integer value is :" +a);
        System.out.println("Enter your name");
        String b = sc1.nextLine();
        System.out.println("Given name is " +b);
        Scanner sc2 = new Scanner(System.in);
        System.out.println("Enter the temp in feh");
       Double feh = sc2.nextDouble();
       convertIntoCesius(feh);
    }
public static void convertIntoCesius(Double feh)
        {
        Double cel = (5 *(feh -32)/9);
        System.out.println("Celsius temp is : " +cel);
        }
}

